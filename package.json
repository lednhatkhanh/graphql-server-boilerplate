{
  "name": "graphql-server-boilerplate",
  "version": "1.0.0",
  "description": "A simple Graphql server boilerplate",
  "scripts": {
    "dev": "nodemon",
    "build": "babel src --out-dir build --extensions \".ts\"",
    "clean": "rimraf build",
    "lint": "eslint --ext .ts src",
    "start": "node build/index.js",
    "postinstall": "npm run prisma:generate",
    "prisma:migrate:up": "dotenv -e .env.local prisma migrate up -- --experimental",
    "prisma:migrate:save": "dotenv -e .env.local prisma migrate save -- --experimental",
    "prisma:introspect": "dotenv -e .env.local prisma introspect",
    "prisma:studio": "dotenv -e .env.local prisma studio",
    "prisma:generate": "prisma generate"
  },
  "keywords": [],
  "author": "Khanh Le",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.12.8",
    "@nexus/schema": "^0.19.1",
    "@prisma/client": "2.12.0",
    "apollo-server-express": "^2.19.0",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.4.0",
    "jsonwebtoken": "^8.5.1",
    "nexus-plugin-prisma": "^0.25.0",
    "graphql-scalars": "^1.5.0",
    "luxon": "^1.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/runtime": "^7.12.5",
    "@prisma/cli": "2.12.0",
    "@types/bcrypt": "^3.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/luxon": "^1.25.0",
    "@types/node": "^12.19.7",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.14.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2"
  }
}
